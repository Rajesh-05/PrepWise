# MongoDB Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/prepwise?retryWrites=true&w=majority
MONGODB_DBNAME=prepwise

# Frontend URL (for CORS and OAuth redirects)
# Set to your actual frontend URL in production
FRONTEND_URL=http://localhost:3000

# Google OAuth Configuration (get from Google Cloud Console)
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:5000/auth/google/callback

# Gemini AI API Key (get from Google AI Studio)
GEMINI_API_KEY=your-gemini-api-key

# Vapi AI Configuration (for mock interviews)
VAPI_PRIVATE_KEY=your-vapi-private-key

# JWT Configuration
JWT_SECRET=your-super-secret-key-change-this-in-production
JWT_EXPIRES_MIN=10080

# Logging Level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# Flask Environment
FLASK_ENV=development
FLASK_DEBUG=0
